$ gcc -print-file-name
@ -print-file-name

code {
for lib in      \
    libc.a      \
    libm.a      \
    crt1.o      \
    crti.o      \
    crtn.o      \
    gcrt1.o     \
    libgcc.a    \
    crtbegin.o  \
    crtend.o    \
    libstdc++.a \
    libstdc++.a
do
   realpath $(gcc -print-file-name=$lib)
done
code }

