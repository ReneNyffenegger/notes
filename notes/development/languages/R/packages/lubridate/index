$ R package: lubridate

{ Convert seconds to hours and minutes

code {
library(lubridate);

for (sec in → development/languages/R/functions/seq[seq](36300, 92000, by=7*60)) {
   sec_p <- seconds_to_period(sec);
   hr_mi <- sprintf('%02d:%02d', sec_p@hour, minute(sec_p));
   print(hr_mi);
}
code }

}

sa:
  → development/languages/R/packages
