{ csv

code {
[out:csv(
   num, txt ;    // Column names
   true     ;    // header (true is default)
  "|"            // separator
)];

make foo   // <--- the type / if output is xml, it becomes the XML's tag name
     num = 42,
     txt ='Hello world';

out;
code }

}
{ bbox

code {
[bbox:{{bbox}}];

nw[amenity=restaurant];

out center;
code }

}

