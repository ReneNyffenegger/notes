code {
create database tq84_db;
code }

code {
create user rene@localhost identified by 'rene';
create user rene@'%'       identified by 'rene';
code }

code {
grant all on tq84_db.* to rene@localhost;
grant all on tq84_db.* to rene@'%';
code }

code {
select * from sys.version;
code }

