$ SQL Server - sys.dm_os_memory_objects
-
@ sys.dm_os_memory_objects

code {
select
   memory_object_address,
   parent_address,
   pages_in_bytes,
   creation_options,
   bytes_used,
   type,
   name,
   memory_node_id,
   creation_time,
   page_size_in_bytes,
   max_pages_in_bytes,
   page_allocator_address,
   creation_stack_address,
   sequence_num,
   partition_type,
   partition_type_desc,
   contention_factor,
   waiting_tasks_count,
   exclusive_access_count
from
   sys.dm_os_memory_objects
code }

{ Object types

code {
select
   type
-- COUNT(*)
from
   sys.dm_os_memory_objects
group by
   type
order by
   type
code }
