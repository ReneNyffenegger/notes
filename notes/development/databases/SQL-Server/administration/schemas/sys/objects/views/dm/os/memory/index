$ SQL Server: sys.dm_os_memory_* views

code {
select
   obj.name
from
   sys.schemas         sch join
   sys.all_objects     obj on sch.schema_id = obj.schema_id
where
   sch.name      = 'sys'             and
   obj.type_desc = 'VIEW'            and
   obj.name like   'dm_os_memory_%'
order by
   obj.name
;
code }

