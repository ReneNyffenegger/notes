$ msdb.dbo.sysschedules

code {
select
   sch.schedule_id,
   sch.name                     schedule_name,
   sch.freq_type,
   sch.freq_interval,
   sch.freq_subday_type,
   sch.freq_subday_interval,
   sch.freq_relative_interval,
   sch.freq_recurrence_factor,
   sch.active_start_time,
   sch.active_end_time,
   sch.originating_server_id,
   suser_sname(sch.owner_sid)   owner,
   sch.date_created,
   sch.date_modified,
   sch.version_number
from
   msdb.dbo.sysschedules sch
code }

