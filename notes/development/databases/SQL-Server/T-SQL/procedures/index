$ T-SQL: procedures

code {
create procedure dbo.my_procedure ( -- Note: the parantheses around the parameters are optional. I use it only for visual purposes.
      @param_one nvarchar(42),
      @param_two nvarchar(20),
      @param_out int           output
)
as
  set nocount on
  
  select …
  where
    field_one = @param_one and
    field_two = @param_two;

  set @param_out = 42;

  set nocount off
code }

code {
declare @ret int;
exec dbo.my_procedure('foo', 'bar', @ret out);
code }



