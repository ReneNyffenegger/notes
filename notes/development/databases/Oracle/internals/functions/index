code {
create or replace function tq84_kecr_os_seconds return binary_integer is
   language  c
   name     "kecr_OS_seconds"
   library   dbms_workload_capture_lib
   with context
   parameters(context) ;
/

select
   tq84_kecr_OS_seconds - round((sysdate - date '1970-01-01') * 86400) + 1 zero
from
   dual
code }



