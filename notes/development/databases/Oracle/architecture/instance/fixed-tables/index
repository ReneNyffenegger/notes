{ Perl script to find x$ tables

code {
#!/usr/bin/perl -w

use strict;

open my $ora , '<', $ENV{ORACLE_HOME} . "/bin/oracle.exe" or die 'could not open oracle.exe';
open my $xout, '>', 'x.out' or die;

my $l;
my $p = ' ' x 40;
my %x;

while (read ($ora, $l, 10000)) {
   $l = $p.$l;

   foreach ($l =~ /(x\$\w{3,})/g) {
      $x{$_}++;
   }

   $p = substr ($l,-40);
}

foreach (sort keys %x) {
   print $xout "$_\n";
}
code }

}




