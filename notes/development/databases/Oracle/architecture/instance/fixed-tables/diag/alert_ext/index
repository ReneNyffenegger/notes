$ Oracle: X$DIAG_ALERT_EXT
@ x$diag_alert_ext

code {
select
   originating_timestamp,
   normalized_timestamp,
   message_group,
   message_text,
   operation_id,
   cause_text,
   component_id,
   message_type,
   message_level,
   message_id
from
   x$diag_alert_ext
order by
   indx desc;
code }

