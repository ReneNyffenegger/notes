$ Oracle: X$KSLLW
@ x$ksllw

code {
select
   ws.ksllasnam "parent_name",
   lw.ksllwnam  "location",
   lw.ksllwlbl  "unit to guard"
from
   x$ksllw  lw                        join
   x$kslwsc ws on lw .indx = ws.indx
-- where
--     lw.ksllwnam like 'kcbgtcr%'                  and
--     ws.ksllasnam      = 'cache buffers lru chain'
order by
   ws.ksllasnam,
   lw.ksllwlbl
;
code }

sa:
