   fe.fecrc_rba_bof,
   fe.fecrc_etb,
   fe.fecps,
   fe.fepor,
   fe.fetsn,
   fe.fetsi,
   fe.ferfn,
   fe.fepft,
   fe.fedor,
   fe.fepdi,
   fe.fefdb,
   fe.feplg_scn,
   fe.fepax,
   fe.feflg,
   fe.fepfp,
   fe.feplus,
   fe.feprls,
   fe.feprlt,
   fe.fefcrs,
   fe.fefcrt,
   fe.fefcps,
   fe.fecpt,
   fe.femvst,
   fe.fepfdi,
   fe.fepfcps,
   fe.fepfrls,
   fe.fepfrlt,
   fe.feibrft,
   fe.ferfno
from
