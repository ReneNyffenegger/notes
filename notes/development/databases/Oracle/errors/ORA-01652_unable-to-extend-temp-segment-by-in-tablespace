$ ORA-01652: unable to extend temp segment by … in tablespace

code {
select
   block_size,
   round(max_size / 1024 / 1024, 2) max_mb,
   max_extents,
   status,
   contents,
   extent_management,
   allocation_type,
   segment_space_management
from
   dba_tablespaces
where
   tablespace_name = '…'
;
code }
