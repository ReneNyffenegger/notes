$ Oracle SQL Plan operation FAST DUAL

code {
explain plan for
   select * from dual;
   
select * from table(dbms_xplan.display(format=>'basic'));
--
-- ----------------------------------
-- | Id  | Operation         | Name |
-- ----------------------------------
-- |   0 | SELECT STATEMENT  |      |
-- |   1 |  TABLE ACCESS FULL| DUAL |
-- ----------------------------------

explain plan for
   select sysdate from dual;
   
select * from table(dbms_xplan.display(format=>'basic'));
--
-- ---------------------------------
-- | Id  | Operation        | Name |
-- ---------------------------------
-- |   0 | SELECT STATEMENT |      |
-- |   1 |  FAST DUAL       |      |
-- ---------------------------------
code }

