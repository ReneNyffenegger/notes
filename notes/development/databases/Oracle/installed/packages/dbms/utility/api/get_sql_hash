$ Oracle: DBMS_UTILITY.GET_SQL_HASH

code {
declare
   h number;
   r raw(100);
   p number;
begin
   h := dbms_utility.get_sql_hash('select 42 from dual', r, p);
   dbms_output.put_line('h = ' || h);
end;
/
code }

