$ Oracle: DBMS_ASSERT
@ dbms_assert

{ Functions

  table { ll

    `enquote_literal` ☰
    `enquote_name` ☰
    `noop` ☰
    `qualified_sql_name` ☰
    `schema_name` ☰
    `simple_sql_name` ☰
    `simple_sql_name_list` ☰
    `sql_object_name` ☰

  table }

}
{ Examples

code {
select dbms_assert.enquote_name('foo'    ) from dual;
-- "FOO"
select dbms_assert.enquote_name('foo.bar') from dual;
-- "FOO.BAR"
code }

code {
select dbms_assert.enquote_literal('foo.bar') from dual;
-- 'foo.bar'
code }

}

sa:
  → development/databases/Oracle/installed/packages/dbms
