$ Oracle: DBA_FLASHBACK_ARCHIVE_TS
@ dba_flashback_archive_ts

table { ll
select
   tablespace_name,
   flashback_archive_name,
   flashback_archive#,
   quota_in_mb            / 1024 quota_in_gb
from
   dba_flashback_archive_ts;
table }

