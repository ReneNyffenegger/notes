$ Oracle: DBA_FLASHBACK_ARCHIVE
@ dba_flashback_archive

code {
select
   owner_name,
   flashback_archive_name,
   retention_in_days,
   create_time,
   last_purge_time,
   status,
   flashback_archive#
from
   dba_flashback_archive;
code }

