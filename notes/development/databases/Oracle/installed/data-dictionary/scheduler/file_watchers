$ Oracle: DBA_SCHEDULER_FILE_WATCHERS
@ dba_scheduler_file_watchers

code {
select
   owner,
   file_watcher_name,
   enabled,
   destination_owner,
   destination,
   directory_path,
   file_name,
   credential_name,
   min_file_size,
   steady_state_duration,
   last_modified_time,
   comments
from
   dba_scheduler_file_watchers;
code }

