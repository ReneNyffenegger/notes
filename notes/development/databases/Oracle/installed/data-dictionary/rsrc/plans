$ Oracle: DBA_RSRC_PLANS
@ dba_rsrc_plans

code {
select
   rpl.plan,
   rpl.status,
   rpl.cpu_method,
   rpl.mgmt_method,
   rpl.mandatory,
   rpl.active_sess_pool_mth,
   rpl.parallel_degree_limit_mth,
   rpl.queueing_mth,
   rpl.sub_plan,
   rpl.num_plan_directives,
   rpl.comments,
   rpl.plan_id
from
   dba_rsrc_plans rpl;
code }

