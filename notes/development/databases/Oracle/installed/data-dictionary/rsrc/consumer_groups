$ Oracle: DBA_RSRC_CONSUMER_GROUPS
@ dba_rsrc_consumer_groups

code {
select 
   consumer_group_id,
   consumer_group,
   cpu_method,
   mgmt_method,
   category,
   mandatory,
   status,
   internal_use,
   comments
from
   dba_rsrc_consumer_groups
order by
  category;
code }

