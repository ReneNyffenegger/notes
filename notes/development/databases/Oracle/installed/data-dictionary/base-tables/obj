$ Oracle: OBJ$

code {
select
   usr.name   obj_owner,
   obj.name   obj_name,
   case when bitand(obj.flags, 128) <> 0 then 'y' end in_recycle_bin
from
   obj$  obj                             join
   user$ usr on obj.owner# = usr.user#
;
code }

