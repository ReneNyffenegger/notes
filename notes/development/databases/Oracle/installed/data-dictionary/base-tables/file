$ Oracle: SYS.FILE$
@ file$

code {
select
   fil.file#,    -- Join with â†’ development/databases/Oracle/architecture/instance/fixed-tables/k/c/c/f/n[x$kccfn.fnfno] and find filename in `x$kccfn`.
   fil.status$,
   fil.blocks,
   fil.ts#,
   fil.relfile#,
   fil.maxextend,
   fil.inc,
   fil.crscnwrp,
   fil.crscnbas,
   fil.ownerinstance,
   fil.spare1,
   fil.spare2,
   fil.spare3,
   fil.spare4
from
   sys.file$ fil;
code }
