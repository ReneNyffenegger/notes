$ Oracle: DBA_DOMAINS

code {
select
   dom.builtin,
   dom.cols,
   dom.data_display,
   dom.data_order,
   dom.discriminantcols,
   dom.display_length,
   dom.flags,
   dom.name,
   dom.order_length,
   dom.owner,
   dom.selector,
   dom.selector_length
from
   dba_domains dom;
code }

