$ Oracle: DBA_PG_EDGE_RELATIONSHIPS

code {
select
   pge.edge_col_name,
   pge.edge_end,
   pge.edge_tab_name,
   pge.graph_name,
   pge.owner,
   pge.vertex_col_name,
   pge.vertex_tab_name
from
   dba_pg_edge_relationships pge;
code }

