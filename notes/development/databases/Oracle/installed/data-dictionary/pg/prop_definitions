$ Oracle: DBA_PG_PROP_DEFINITIONS
@ dba_pg_prop_definitions

code {
select
   property_name,
   column_name,
   column_expr
from
   dba_pg_prop_definitions
where
   owner        = user              and
   graph_name   ='…'       and
   element_name ='…';
code } 

