$ Oracle: DBA_PG_ELEMENTS

code {
select
   pge.element_kind,
   pge.element_name,
   pge.graph_name,
   pge.object_name,
   pge.object_owner,
   pge.owner
from
   dba_pg_elements pge;
code }

