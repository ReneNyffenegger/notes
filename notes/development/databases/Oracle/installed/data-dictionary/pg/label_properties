$ Oracle: DBA_PG_LABEL_PROPERTIES

code {
select
   plp.character_set_name,
   plp.collation,
   plp.data_char_length,
   plp.data_length,
   plp.data_precision,
   plp.data_scale,
   plp.data_type,
   plp.graph_name,
   plp.label_name,
   plp.owner,
   plp.property_name,
   plp.property_order
from
   dba_pg_label_properties plp;
code }

