$ Oracle: DBA_DOMAIN_CONSTRAINTS

code {
select
   dom.bad,
   dom.constraint_type,
   dom.deferrable,
   dom.deferred,
   dom.domain_name,
   dom.domain_owner,
   dom.generated,
   dom.invalid,
   dom.name,
   dom.origin_con_id,
   dom.rely,
   dom.search_condition,
   dom.status,
   dom.validated
from
   dba_domain_constraints dom;
code }

