   IUnknown     *punkVal;
   IDispatch    *pdispVal;
   SAFEARRAY    *parray;
   BYTE         *pbVal;
   SHORT        *piVal;
   LONG         *plVal;
   LONGLONG     *pllVal;
   FLOAT        *pfltVal;
   DOUBLE       *pdblVal;
   VARIANT_BOOL *pboolVal;

   SCODE        *pscode;
   CY           *pcyVal;
   DATE         *pdate;
   BSTR         *pbstrVal;
   IUnknown    **ppunkVal;
   IDispatch   **ppdispVal;
   SAFEARRAY   **pparray;
   VARIANT      *pvarVal;
   PVOID         byref;
   CHAR          cVal;
   USHORT        uiVal;
   ULONG         ulVal;
   ULONGLONG     ullVal;
   INT           intVal;
   UINT          uintVal;
   DECIMAL      *pdecVal;
   CHAR         *pcVal;
   USHORT       *puiVal;
   ULONG        *pulVal;
   ULONGLONG    *pullVal;
   INT          *pintVal;
   UINT         *puintVal;
   struct {
     PVOID        pvRecord;
     IRecordInfo *pRecInfo;
   } ;
 } ;
      } ;
      DECIMAL decVal;
    } ;
  };

typedef VARIANT *LPVARIANT;
typedef VARIANT  VARIANTARG;
typedef VARIANT *LPVARIANTARG;
code }

