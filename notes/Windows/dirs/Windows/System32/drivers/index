  This list was produces with the following → Windows/PowerShell script
code {
set-strictMode -version latest

function printIfBelow($below, $path, $module, $description, $drivers, $startMode) {
   #
   #   Display only files that are found in C:\Windows\System32\drivers
   #  (Some are also in C:\WINDOWS\system32\DriverStore
   #
   if ($path -match $below)  {

       $path = $path -replace 'c:\\windows\\system32\\drivers\\', ''
#     '{0,-43} | {1,-34} | {2,-80} | {3,-12} | {4,-8}' -f $path, $moduleName, $description, $driverType, $startMode
      '{0,-43} | {2,-80} | {3,-12} | {4,-8}'           -f $path, $moduleName, $description, $driverType, $startMode

   }
}

function showDriversPerLine {
    begin   {
            #
            #    Only print drivers below this path
            #
                $below = 'c:\\windows\\system32\\drivers\\'

            #
            #    Variables used in script (make set-strictMode happy …)
            #
                $moduleName  = ''
                $displayName = ''
                $description = ''
                $driverType  = ''
                $startmode   = ''
                $path        = ''
            }

    process {

            if ($_ -eq "") {

               if ($displayName -ne $description) {
                     write-warning "Assumption displayName = description turned out to be wrong: $displayName <> $description"
               }

               printIfBelow  "$below" "$path" "$moduleName" "$description" "$driverType" "$startMode"

               $moduleName  = ''
               $displayName = ''
               $description = ''
               $driverType  = ''
               $startmode   = ''
               $path        = ''

            }
            else {

               if     ($_ -match '^Module name:' ) { $moduleName  = $_.substring(19) }
               elseif ($_ -match '^Display Name:') { $displayName = $_.substring(19) }
               elseif ($_ -match '^Description:' ) { $description = $_.substring(19) }
               elseif ($_ -match '^Driver Type:' ) { $driverType  = $_.substring(19) }
               elseif ($_ -match '^Start Mode:'  ) { $startMode   = $_.substring(19) }
               elseif ($_ -match '^Path:'        ) { $path        = $_.substring(19) }

            }

            }

    end     {
          #
          # last driver is not recognized by an empty line:
          #
            printIfBelow "$below" "$path" "$moduleName" "$description" "$driverType" "$startMode"

            }
}


driverquery /v /fo list | showDriversPerLine
code }

 See also → Windows/PowerShell/language/function/begin-process-end-block[`begin`, `process` and `end` blocks in PowerShell functions]

}

