$ cmd.exe: for /f

code {
for /f %%X in (one.txt two.txt three.txt) do command
for /f %%X in ("Some text etc etc etc"  ) do command
for /f %%X in ('→ Windows/dirs/Windows/System32/cmd_exe/commands/for/f/command[command]'                ) do command
for /f "delims=X" in   …
for /f "eol=X"    in   …
for /f "tokens=n" in   …
for /f "skip=n"   in   …
for /f "→ Windows/dirs/Windows/System32/cmd_exe/commands/for/f/command#cmd-for-f-usebackq[usebackq]" in (`command`) 
code }


