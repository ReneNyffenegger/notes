$ cmd.exe - assoc
@ assoc

code {
C:\> assoc .vbs
.vbs=VBSFile
code }

code {
C:\> assoc VBSFile
VBSFILE=VBScript Script File
code }


code {
C:\> → Windows/cmd-exe/commands/ftype[ftype] VBSFile
VBSFile="→ Windows/dirs/Windows[%SystemRoot%]\System32\WScript.exe "%1" %*
code }

sa:
  → Windows/cmd-exe/commands
