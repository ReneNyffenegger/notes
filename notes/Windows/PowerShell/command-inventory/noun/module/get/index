$ PowerShell cmdLet get-module

code {
get-module packageManagement |
  → Windows/PowerShell/command-inventory/noun/object/select[select-object]  `
     path,
     moduleBase,
     rootModule,
  #  exportedCommands,
     description,
     projectURI,
     version,
     moduleType,
     author,
     clrVersion,
     powerShellVersion |
   → Windows/PowerShell/command-inventory/noun/list/format[format-list]
code }

sa:
  → Windows/PowerShell/command-inventory/noun/module
